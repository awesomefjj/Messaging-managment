<p id="notice"><%= notice %></p>

<h1>Monitorings</h1>

<table>
  <thead>
    <tr>
      <th>Tenant</th>
      <th>Receive</th>
      <th>Receive type</th>
      <th>Kind</th>
      <th>Title</th>
      <th>Content</th>
      <th>Event</th>
      <th>Extra data</th>
      <th>Deleted t</th>
      <th>Created t</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @monitorings.each do |monitoring| %>
      <tr>
        <td><%= monitoring.tenant_id %></td>
        <td><%= monitoring.receive_id %></td>
        <td><%= monitoring.receive_type %></td>
        <td><%= monitoring.kind %></td>
        <td><%= monitoring.title %></td>
        <td><%= monitoring.content %></td>
        <td><%= monitoring.event_id %></td>
        <td><%= monitoring.extra_data %></td>
        <td><%= monitoring.deleted_t %></td>
        <td><%= monitoring.created_t %></td>
        <td><%= link_to 'Show', monitoring %></td>
        <td><%= link_to 'Edit', edit_monitoring_path(monitoring) %></td>
        <td><%= link_to 'Destroy', monitoring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Monitoring', new_monitoring_path %>
